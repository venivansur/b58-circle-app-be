{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node",
        "config": {
          "maxDuration": 10
        }
      }
    ],
    "hooks": {
      "build": "npm run prisma-generate"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.ts"
      }
    ]
  }
  